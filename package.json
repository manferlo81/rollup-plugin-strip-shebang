{
  "name": "rollup-plugin-strip-shebang",
  "version": "1.2.7",
  "description": "A Rollup.js plugin to remove and optionally extract shebang.",
  "main": "dist/strip-shebang.js",
  "module": "dist/strip-shebang.mjs",
  "bundlib": {
    "esModule": true,
    "interop": true,
    "equals": true
  },
  "types": "types/index.d.ts",
  "typings": "types/index.d.ts",
  "keywords": [
    "rollup",
    "plugin",
    "rollup-plugin",
    "shebang",
    "hashbang",
    "extract",
    "strip"
  ],
  "scripts": {
    "clean": "rm -rf node_modules/.cache/bundlib node_modules/.cache/jest dist types coverage",
    "lint": "eslint src/** __test__/**",
    "dev": "bundlib -dw",
    "build": "bundlib -d",
    "test": "jest --color -w 3",
    "prerelease": "npm run clean && npm test && npm run lint && npm run build",
    "release": "standard-version"
  },
  "dependencies": {
    "is-callable": "^1.1.5",
    "magic-string": "^0.25.6",
    "rollup-pluginutils": "^2.8.2"
  },
  "peerDependencies": {
    "rollup": "^1.0.0"
  },
  "devDependencies": {
    "@types/is-callable": "^1.1.0",
    "@types/jest": "^25.1.0",
    "@types/node": "^13.5.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "bundlib": "^0.14.1",
    "chokidar": "^3.3.1",
    "eslint": "^6.8.0",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "standard-version": "^7.1.0",
    "ts-jest": "^25.0.0",
    "typescript": "^3.7.5"
  },
  "files": [
    "dist",
    "types"
  ],
  "author": "Manuel Fernandez <manferlo81@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manferlo81/rollup-plugin-strip-shebang.git"
  },
  "bugs": {
    "url": "https://github.com/manferlo81/rollup-plugin-strip-shebang/issues"
  },
  "homepage": "https://github.com/manferlo81/rollup-plugin-strip-shebang#readme",
  "license": "MIT"
}
