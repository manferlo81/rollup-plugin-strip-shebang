{
  "name": "rollup-plugin-strip-shebang",
  "version": "1.1.4",
  "description": "A Rollup.js plugin to remove and optionally extract shebang.",
  "main": "dist/plugin.cjs.js",
  "module": "dist/plugin.es.js",
  "typings": "types",
  "keywords": [
    "rollup",
    "plugin",
    "rollup-plugin",
    "shebang",
    "hashbang",
    "extract",
    "strip"
  ],
  "scripts": {
    "lint": "tslint src/**/*.ts",
    "pretest": "npm run build",
    "test": "jest",
    "clean": "rm -rf dist && rm -rf types",
    "dev": "rollup -cw",
    "build": "npm run lint && npm run clean && rollup -c",
    "prepublishOnly": "npm test"
  },
  "dependencies": {
    "magic-string": "^0.25.2",
    "rollup-pluginutils": "^2.6.0"
  },
  "peerDependencies": {
    "rollup": "^1.6.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.2",
    "jest": "^24.7.1",
    "rollup": "^1.9.2",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-terser": "^4.0.4",
    "rollup-plugin-typescript2": "^0.20.1",
    "tslint": "^5.15.0",
    "typescript": "^3.4.3"
  },
  "files": [
    "src",
    "dist",
    "types"
  ],
  "author": "Manuel Fernandez <manferlo81@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manferlo81/rollup-plugin-strip-shebang.git"
  },
  "bugs": {
    "url": "https://github.com/manferlo81/rollup-plugin-strip-shebang/issues"
  },
  "homepage": "https://github.com/manferlo81/rollup-plugin-strip-shebang#readme",
  "license": "MIT"
}
