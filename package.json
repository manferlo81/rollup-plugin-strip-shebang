{
  "name": "rollup-plugin-strip-shebang",
  "version": "1.2.9",
  "description": "A Rollup.js plugin to remove and optionally extract shebang.",
  "keywords": [
    "rollup",
    "plugin",
    "rollup-plugin",
    "shebang",
    "hashbang",
    "extract",
    "strip"
  ],
  "main": "dist/strip-shebang.js",
  "module": "dist/strip-shebang.mjs",
  "bundlib": {
    "esModule": true,
    "interop": true
  },
  "types": "types/index.d.ts",
  "typings": "types/index.d.ts",
  "scripts": {
    "clean": "rm -rf node_modules/.cache/bundlib node_modules/.cache/jest dist types coverage",
    "lint": "eslint src/** __test__/**",
    "dev": "bundlib -dw",
    "build": "bundlib -d",
    "test": "jest --color -w 3",
    "prerelease": "npm run clean && npm test && npm run lint && npm run build",
    "release": "standard-version"
  },
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "@rollup/pluginutils": "^3.0.10",
    "is-callable": "^1.1.5",
    "magic-string": "^0.25.7"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@types/is-callable": "^1.1.0",
    "@types/jest": "^25.2.2",
    "@types/node": "^14.0.1",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "bundlib": "^0.16.3",
    "chokidar": "^3.4.0",
    "eslint": "^6.8.0",
    "husky": "^4.2.5",
    "jest": "^25.5.4",
    "rollup": "^2.10.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-export-equals": "^0.3.6",
    "rollup-plugin-typescript2": "^0.27.1",
    "standard-version": "^8.0.0",
    "ts-jest": "^25.5.1",
    "typescript": "^3.9.2"
  },
  "peerDependencies": {
    "rollup": "^2.0.0"
  },
  "files": [
    "dist",
    "types"
  ],
  "author": {
    "name": "Manuel Fernandez",
    "email": "manferlo81@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manferlo81/rollup-plugin-strip-shebang.git"
  },
  "bugs": {
    "url": "https://github.com/manferlo81/rollup-plugin-strip-shebang/issues"
  },
  "homepage": "https://github.com/manferlo81/rollup-plugin-strip-shebang#readme",
  "license": "MIT"
}
