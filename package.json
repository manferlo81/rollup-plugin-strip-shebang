{
  "name": "rollup-plugin-strip-shebang",
  "version": "1.2.9",
  "description": "A Rollup.js plugin to remove and optionally extract shebang.",
  "keywords": [
    "rollup",
    "plugin",
    "rollup-plugin",
    "shebang",
    "hashbang",
    "extract",
    "strip"
  ],
  "main": "dist/strip-shebang.js",
  "module": "dist/strip-shebang.mjs",
  "types": "types/index.d.ts",
  "typings": "types/index.d.ts",
  "scripts": {
    "clean": "rm -rf node_modules/.cache/bundlib node_modules/.cache/jest dist types coverage",
    "lint": "eslint src/** __test__/**",
    "dev": "bundlib -dw",
    "build": "bundlib -d",
    "test": "jest --color -w 3",
    "prerelease": "npm run clean && npm run lint && npm test && npm run build",
    "release": "standard-version"
  },
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "@rollup/pluginutils": "^3.1.0",
    "is-callable": "^1.2.0",
    "magic-string": "^0.25.7"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@types/is-callable": "^1.1.0",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.8.0",
    "bundlib": "^0.17.2",
    "chokidar": "^3.4.2",
    "eslint": "^7.6.0",
    "husky": "^4.2.5",
    "jest": "^26.3.0",
    "standard-version": "^8.0.2",
    "ts-jest": "^26.1.4",
    "typescript": "^3.9.7"
  },
  "peerDependencies": {
    "rollup": "^2.0.0"
  },
  "files": [
    "dist",
    "types"
  ],
  "author": {
    "name": "Manuel Fernandez",
    "email": "manferlo81@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manferlo81/rollup-plugin-strip-shebang.git"
  },
  "bugs": {
    "url": "https://github.com/manferlo81/rollup-plugin-strip-shebang/issues"
  },
  "homepage": "https://github.com/manferlo81/rollup-plugin-strip-shebang#readme",
  "license": "MIT"
}
