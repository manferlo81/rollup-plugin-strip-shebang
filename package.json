{
  "name": "rollup-plugin-strip-shebang",
  "version": "1.2.1",
  "description": "A Rollup.js plugin to remove and optionally extract shebang.",
  "main": "dist/plugin.cjs.js",
  "module": "dist/plugin.es.js",
  "bundlib": {
    "equals": true
  },
  "typings": "types/index.d.ts",
  "keywords": [
    "rollup",
    "plugin",
    "rollup-plugin",
    "shebang",
    "hashbang",
    "extract",
    "strip"
  ],
  "scripts": {
    "lint": "tslint src/**/*.ts",
    "clean": "rm -rf .cache dist types coverage",
    "dev": "bundlib -dw",
    "build": "npm run lint && bundlib -d",
    "test": "jest --color",
    "preversion": "npm run clean && npm run build && npm test"
  },
  "dependencies": {
    "magic-string": "^0.25.2",
    "rollup-pluginutils": "^2.8.1"
  },
  "peerDependencies": {
    "rollup": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.8",
    "bundlib": "^0.5.1",
    "eslint": "^6.0.0",
    "jest": "^24.8.0",
    "tslint": "^5.17.0",
    "typescript": "^3.5.2"
  },
  "files": [
    "src",
    "dist",
    "types"
  ],
  "author": "Manuel Fernandez <manferlo81@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manferlo81/rollup-plugin-strip-shebang.git"
  },
  "bugs": {
    "url": "https://github.com/manferlo81/rollup-plugin-strip-shebang/issues"
  },
  "homepage": "https://github.com/manferlo81/rollup-plugin-strip-shebang#readme",
  "license": "MIT"
}
